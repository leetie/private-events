
<h2><%= @user.name %></h2><br />

<h3>Your Username</h3>
<p><strong><%= @user.username %></strong></p><br />
<%= link_to "Edit Info", edit_user_path(@user) %>
<h3>Upcoming Events</h3>
<% @upcoming_events.all.each do |e| %>
  <%= link_to "#{e.title}", event_path(e) %><br />
<% end %>
<h3>Previous Events</h3>
<% @previous_events.all.each do |e| %>
  <%= link_to "#{e.title}", event_path(e) %><br />
<% end %>

